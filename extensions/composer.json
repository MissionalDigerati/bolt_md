{
    "repositories": {
        "packagist": false,
        "bolt": {
            "type": "composer",
            "url": "https://extensions.bolt.cm/satis/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "preferred-install": "dist"
    },
    "provide": {
        "bolt/bolt": "2.2.24"
    },
    "extra": {
        "bolt-web-path": ".././"
    },
    "autoload": {
        "psr-4": {
            "Bolt\\Composer\\": "",
            "Bolt\\Extension\\MissionalDigerati\\RelatedSort\\": "local/missionaldigerati/boltutilitybox/",
            "Bolt\\Extension\\MissionalDigerati\\BoltRelatedSort\\": "local/missionaldigerati/boltrelatedsort/",
            "Bolt\\Extension\\MissionalDigerati\\BoltClassManager\\": "local/missionaldigerati/boltclassmanager/",
            "Bolt\\Extension\\MissionalDigerati\\BoltPiconSelector\\": "local/missionaldigerati/boltpiconselector/",
            "Bolt\\Extension\\MissionalDigerati\\BoltPrivateContent\\": "local/missionaldigerati/boltprivatecontent/"
        }
    },
    "scripts": {
        "post-package-install": "Bolt\\Composer\\ExtensionInstaller::handle",
        "post-package-update": "Bolt\\Composer\\ExtensionInstaller::handle"
    },
    "require": {
        "bolt/googleanalytics": "^1.0",
        "bolt/simpleforms": "^1.3"
    }
}
